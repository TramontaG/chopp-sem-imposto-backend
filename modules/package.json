{
  "name": "kozz-module-scaffold",
  "version": "1.0.0",
  "description": "Scaffolding to build new kozz modules",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --extensions .ts,.tsx --copy-files --source-maps",
    "dev": "npm run build -- --watch & node --watch --watch-path=dist --env-file=.env --enable-source-maps dist/index.js",
    "start": "npm run build && node --env-file=.env dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kozz-Bot/kozz-module-scaffolding.git"
  },
  "author": "TramontaG",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/mime-types": "^3.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^7.6.0",
    "kozz-types": "^0.1.22",
    "nodemon": "^2.0.22"
  },
  "bugs": {
    "url": "https://github.com/Kozz-Bot/kozz-module-scaffolding/issues"
  },
  "homepage": "https://github.com/Kozz-Bot/kozz-module-scaffolding#readme",
  "dependencies": {
    "axios": "^1.12.1",
    "cookie-parser": "^1.4.7",
    "csv": "^6.4.1",
    "csv-parser": "^3.2.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.5.0",
    "kozz-module-maker": "^0.3.0",
    "mime-types": "^3.0.1",
    "yaseq": "^1.0.2",
    "yasms": "^1.0.2",
    "zod": "^4.1.8"
  }
}
